
<input type="button"  class="btn btn-small btn-primary"  value="New Province" data-bind="click:$root.CreateProvince" />
<hr />
<table class="table table-striped table-bordered table-condensed">
    <tr> 
        <th>Name</th>
        <th></th>
    </tr>
    <tbody data-bind="foreach: ProvincesArray">
        <tr>
            <td class="name"><a data-bind="text: ProvinceName, click: $parent.EditProvince"></a></td>
            <td><button class="btn btn-mini btn-danger" data-bind="click: $parent.RemoveProvince">remove</button></td>
        </tr>
    </tbody>
</table>

<script src="~/Scripts/Province.js"></script>